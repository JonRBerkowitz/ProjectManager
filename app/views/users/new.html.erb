<h1>Sign Up</h1>
<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password %>
  <%= f.text_field :password %><br>
  <%= f.submit 'Submit' %>
   <% @user.errors.full_messages.each do |msg| %>
     <p><%= msg %></p>
   <% end %>
<% end %>
<%= link_to('Already Have an Account? Sign in Here.', login_path) %>
